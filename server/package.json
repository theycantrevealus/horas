{
  "name": "server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build --webpack --webpackPath webpack.config.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:webpack": "rm -rf dist && nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "typeorm:migration:run": "npm run typeorm migration:run -- -d src/configs/postgresql.datasource.ts",
    "typeorm:migration:create": "npm run typeorm -- migration:create src/migrations/$npm_config_name",
    "typeorm:migration:generate": "npm run typeorm -- -d src/configs/postgresql.datasource.ts migration:generate src/migrations/$npm_config_name",
    "typeorm:migration:revert": "npm run typeorm -- -d src/configs/postgresql.datasource.ts migration:revert",
    "typeorm:seed:generate": "ts-node typeorm migration:generate -c seed -n ",
    "typeorm:seed:run": "ts-node typeorm migration:run -c seed",
    "typeorm:seed:revert": "ts-node typeorm migration:revert -c seed",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:parameter": "echo src/migrations/$npm_config_name"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.11",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.11",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.11",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/swagger": "^6.1.2",
    "@nestjs/typeorm": "^9.0.1",
    "@sentry/node": "^7.13.0",
    "@sentry/tracing": "^7.13.0",
    "@types/passport-jwt": "^3.0.6",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "copy-webpack-plugin": "^11.0.0",
    "dotenv": "^16.0.2",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "fs": "^0.0.1-security",
    "i": "^0.3.7",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.8.0",
    "raw-body": "^2.5.1",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^3.3.0",
    "rimraf": "^3.0.2",
    "sequelize-typescript": "^2.1.3",
    "typeorm": "0.3.9",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.3",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.0.11",
    "@sentry/minimal": "^6.19.7",
    "@sentry/node": "^7.8.0",
    "@types/express": "^4.17.14",
    "@types/jest": "29.0.3",
    "@types/node": "^18.7.18",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.0.3",
    "prettier": "^2.7.1",
    "run-script-webpack-plugin": "^0.1.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.4",
    "ts-jest": "29.0.1",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.3",
    "webpack": "^5.74.0",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
