  "scripts": {
    "build": "nest build core",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev:core": "nest build core --webpack --webpackPath ./webpack.config.js --watch",
    "start:dev:account": "nest build account --webpack --webpackPath ./webpack.config.js --watch",
    "start:dev:inventory": "nest build inventory --webpack --webpackPath ./webpack.config.js --watch",
    "start:dev:queue": "nest build queue --webpack --webpackPath ./webpack.config.js --watch",
    "start:dev:import": "nest build import --webpack --webpackPath ./webpack.config.js --watch",
    "start:dev:socket": "nest build socket --webpack --webpackPath ./webpack.config.js --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/server/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config ./jest-all.json --runInBand --coverage --no-cache",
    "test:watch": "jest --config ./jest-all.json --watch --verbose --runInBand",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./jest-e2e.json --runInBand --coverage --no-cache",
    "test:e2e:watch": "jest --config ./jest-e2e.json --watchAll --verbose --runInBand --coverage --no-cache",
    "test:unit": "jest --config ./jest-unit.json --runInBand --coverage --no-cache",
    "test:unit:watch": "jest --config ./jest-unit.json --watchAll --verbose --runInBand --coverage --no-cache",
    "test:cov": "jest --config ./jest-all.json --watch --verbose --runInBand --coverage --no-cache"
  },
