{
  "name": "server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build core",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev:core": "nest build core --webpack --webpackPath ./webpack.config.js --watch",
    "start:dev:inventory": "nest build inventory --webpack --webpackPath ./webpack.config.js --watch",
    "start:dev:import": "nest build import --webpack --webpackPath ./webpack.config.js --watch",
    "start:dev:socket": "nest build socket --webpack --webpackPath ./webpack.config.js --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/server/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./jest-e2e.json",
    "test:unit": "jest --config ./jest-unit.json --verbose --runInBand",
    "test:unit:watch": "jest --config ./jest-unit.json --watch --verbose --runInBand",
    "test:unit:cov": "jest --config ./jest-unit.json --watch --verbose --runInBand --coverage"
  },
  "dependencies": {
    "@fastify/static": "^6.10.2",
    "@fastify/view": "^8.0.0",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^10.1.1",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.1.1",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/mongoose": "^10.0.1",
    "@nestjs/platform-express": "^10.1.1",
    "@nestjs/platform-socket.io": "^10.1.1",
    "@nestjs/swagger": "^7.1.2",
    "@nestjs/websockets": "^10.1.1",
    "axios": "^1.4.0",
    "cache-manager": "^5.2.3",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "crypto": "^1.0.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "handlebars": "^4.7.7",
    "ioredis": "^5.3.2",
    "lz-string": "^1.5.0",
    "moment-timezone": "^0.5.43",
    "mongoose": "^7.4.1",
    "path": "^0.12.7",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "socket.io-client": "^4.7.1"
  },
  "devDependencies": {
    "@blazity/nest-file-fastify": "^1.0.0",
    "@faker-js/faker": "^8.0.2",
    "@fastify/multipart": "^7.7.3",
    "@golevelup/ts-jest": "^0.4.0",
    "@nestjs/cli": "^10.1.10",
    "@nestjs/microservices": "^10.1.1",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-fastify": "^10.1.1",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.1.1",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.3",
    "@types/multer": "^1.4.7",
    "@types/node": "20.4.4",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "bcrypt": "^5.1.0",
    "cache-manager-ioredis": "^2.1.0",
    "copy-webpack-plugin": "^11.0.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "fastify": "^4.21.0",
    "fs": "^0.0.1-security",
    "jest": "29.6.1",
    "kafkajs": "^2.2.4",
    "mongodb-memory-server": "^8.13.0",
    "multer": "^1.4.5-lts.1",
    "nestjs-soap": "^3.0.2",
    "passport": "^0.6.0",
    "prettier": "^3.0.0",
    "redis": "^4.6.7",
    "request-ip": "^3.3.0",
    "run-script-webpack-plugin": "^0.2.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.1.6",
    "uuid": "^9.0.0",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0",
    "winston": "^3.10.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^@core(.*)$": "<rootDir>/apps/core/src/modules$1",
      "^@inventory(.*)$": "<rootDir>/apps/inventory/src$1",
      "^@log(.*)$": "<rootDir>/apps/log/src$1",
      "^@security(.*)$": "<rootDir>/apps/security$1",
      "^@configuration(.*)$": "<rootDir>/apps/configuration$1",
      "^@utility(.*)$": "<rootDir>/apps/utility$1",
      "^@interceptors(.*)$": "<rootDir>/apps/interceptors$1",
      "^@guards(.*)$": "<rootDir>/apps/guards$1",
      "^@decorators(.*)$": "<rootDir>/apps/decorators$1"
    },
    "slowTestThreshold": 30,
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/"
    ]
  }
}
